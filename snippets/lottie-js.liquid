    var bodyMovinId = 'async-bodymovin-script';
    window.zmAnimations = window.zmAnimations || {
        register: function (opts) {
            if (this.loaded) return this.load(opts);
            zmAnimations[opts.name] = opts;
            this.loadBodyMovin();
        },
        load: function (opts) {
            bodymovin.loadAnimation(opts);
        },
        loadAll: function () {
            zmAnimations.loaded = true;
            var anims = Object.assign({}, zmAnimations);
            delete anims.register;
            delete anims.load;
            for (var n in anims) {
                if (typeof anims[n] === 'object')
                    bodymovin.loadAnimation(anims[n]);
            }
        },
        loadBodyMovin: function () {
            if (!document.getElementById(bodyMovinId)) {
                var s = document.createElement('script');
                s.src = 'https://cdnjs.cloudflare.com/ajax/libs/bodymovin/5.7.4/lottie.min.js';
                s.defer = true;
                s.onload = zmAnimations.loadAll;
                s.id = 'async-bodymovin-script'
                document.body.appendChild(s);
            }
        }
    };
