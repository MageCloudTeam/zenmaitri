}

.giftcard__code__inner{
    display:inline-block;
    vertical-align:baseline;
    background-color:#fff;
    padding:.5em;
    border-radius:4px;
    max-width:450px;
    box-shadow:0 0 0 1px rgba(0, 0, 0, 0.1)
}

.giftcard__code--small .giftcard__code__inner{
    overflow:auto
}

.giftcard__code__text{
    font-weight:400;
    font-size:1.875em;
